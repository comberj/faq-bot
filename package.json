{
	"name": "sean-bot",
	"version": "1.0.0",
	"description": "Sean-Bot",
	"main": "app.js",
	"repository": "https://github.com/comberj/faq-bot",
	"author": "SAM CLI",
	"license": "MIT",
	"dependencies": {},
	"scripts": {
		"api": "sam local start-api --region us-west-2 --parameter-overrides \"$(jq -r -j 'to_entries[] | \"ParameterKey=\\(.key),ParameterValue=\\(.value) \"' config.json)\"",
		"package": "sam package --template-file ./template.yaml --s3-bucket bucketname --output-template-file ./packaged.yaml",
		"deploy": "sam deploy --template-file ./packaged.yaml --stack-name stackname --capabilities CAPABILITY_IAM --parameter-overrides $(jq -r 'to_entries[] | \"\\(.key)=\\(.value)\"' config.json)"
	}
}
